<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/transFragment">
    <fragment
        android:id="@+id/transFragment"
        android:name="com.beginner.translite.TransFragment"
        android:label="TransFragment" >
        <action
            android:id="@+id/action_transFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
        <action
            android:id="@+id/action_transFragment_to_blankFragment"
            app:destination="@id/blankFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.beginner.translite.HistoryFragment"
        android:label="HistoryFragment" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.beginner.translite.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
</navigation>